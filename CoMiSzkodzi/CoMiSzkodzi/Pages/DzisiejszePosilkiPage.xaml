<?xml version="1.0" encoding="utf-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" BackgroundImage="backgroundimage.png" x:Class="CoMiSzkodzi.Pages.DzisiejszePosilkiPage" xmlns:d="clr-namespace:CoMiSzkodzi;assembly=CoMiSzkodzi">
    <ContentPage.Content>
        <StackLayout>
            <StackLayout.Margin>
                <OnPlatform iOS="0,20,0,0" />
            </StackLayout.Margin>
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
